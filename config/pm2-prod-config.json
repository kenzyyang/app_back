{
  "apps": {
    "name": "audio",
    "script": "app.js",
    "cwd": "./",
    "args": "",
    "interpreter": "",
    "interpreter_args": "",
    "watch": true,
    "ignore_watch": [
      "node_modules",
      "logs"
    ],
    "exec_mode": "cluster_mode",
    "instances": 4,
    "max_memory_restart": 8,
    "error_file": "./logs/app-err.log",
    "out_file": "./logs/app-out.log",
    "merge_logs": true,
    "log_date_format": "YYYY-MM-DD HH:mm:ss",
    "min_uptime": 100,
    "max_restarts": 10,
    "autorestart": true,
    "cron_restart": "",
    "restart_delay": 100,
    "env": {
      "NODE_ENV": "prop"
    }
  }
}